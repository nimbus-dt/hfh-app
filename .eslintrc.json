{
  "extends": [
    "wesbos",
    "react-app/jest",
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "plugins": ["@typescript-eslint"],
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"]
      }
    }
  },
  "parser": "@typescript-eslint/parser",
  "rules": {
    "prettier/prettier": 0,
    "@typescript-eslint/no-explicit-any": "error"
  },
  "root": true
}
